<ul class="module-menu">
    <li>
        <a ui-sref="monackfr-usermanagement-users()">Users</a>
    </li>
    <li>
        <a ui-sref="monackfr-usermanagement-groups()">Groups</a>
    </li>
    <li>
        <a ui-sref="monackfr-usermanagement-roles()">Roles</a>
    </li>
</ul>
<div class="module-content">
    <form ng-submit="editUserCtrl.edit()">
        <ul>
            <li>
                <label>Username</label>
                <input type="text" ng-model="editUserCtrl.user.UserName" />
            </li>
            <li>
                <label>Password</label>
                <input type="password" ng-model="editUserCtrl.user.Password" />
            </li>
            <li>
                <label>Email</label>
                <input type="email" ng-model="editUserCtrl.user.Email" />
            </li>
            <li>
                <input type="submit" name="submit" value="Save user" />
            </li>
        </ul>
    </form>
</div>